<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="reportAccount">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="account">
                <t t-call="web.basic_layout">
                    <div class="page">
                        <h2>Account Report</h2>
                        <p><strong>Account Number:</strong> <t t-esc="account.accountNumber"/></p>
                        <p><strong>Balance:</strong> <t t-esc="account.balance"/> Euro</p>
                        <p><strong>Creation Date:</strong> <t t-esc="account.creationDate"/></p>
                        <p><strong>Product ID</strong> <t t-esc="account.productId"/></p>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>
